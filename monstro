ackage labirinto;

import java.util.Scanner;

public class Monster {
   private int health=3;
   private String nome;
   public  int pas;
   public int fase;
   public void worstLife(Boolean result) {
	   if(result) {
		    health=health-1;
	   }else {
		   health=health;
	   }
	   System.out.println(health);
   }
   public void death(){
	   if(health==0){
		   System.out.println(" o monstro morreu");
	   }
   }
  
}
